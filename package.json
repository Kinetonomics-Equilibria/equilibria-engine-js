{
  "name": "equilibria-engine-js",
  "version": "1.0.6",
  "description": "Headless js renderer for kineticgraphs",
  "main": "dist/kgjs.cjs.js",
  "module": "dist/kgjs.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
